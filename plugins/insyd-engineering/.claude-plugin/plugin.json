{
  "name": "insyd-ai-engineering",
  "version": "4.1.0",
  "description": "Engineering plugin for modern JavaScript/TypeScript development with TDD enforcement and sub-agent orchestration. Features 24 agents, 24 commands, 14 skills, and hooks for Test-Driven Development workflow. Includes orchestrated feature development, Ralph autonomous loops with parallel subagents, spec writing (PRD+TDD), functional testing, code review, refactoring, and codebase indexing.",
  "author": {
    "name": "Insyd-AI",
    "url": "https://github.com/insyd-ai"
  },
  "homepage": "https://github.com/insyd-ai/compound-engineering-plugin",
  "repository": "https://github.com/insyd-ai/compound-engineering-plugin",
  "license": "MIT",
  "keywords": [
    "typescript",
    "javascript",
    "nextjs",
    "convex",
    "astro",
    "react",
    "vite",
    "testing",
    "tdd",
    "test-driven-development",
    "bun",
    "playwright",
    "code-review",
    "refactoring",
    "spec-writing",
    "browser-automation",
    "engineering",
    "insyd-ai"
  ],
  "hooks": "./hooks/hooks.json",
  "mcpServers": {
    "pw": {
      "type": "stdio",
      "command": "bunx",
      "args": ["@playwright/mcp@latest"],
      "env": {}
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "convex": {
      "type": "stdio",
      "command": "npx",
      "args": ["convex", "mcp", "start"],
      "env": {}
    },
    "linear": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/sse"],
      "env": {}
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PAT}"
      }
    },
    "shadcn": {
      "type": "stdio",
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "env": {}
    }
  }
}
